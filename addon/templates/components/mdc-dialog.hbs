<div class="mdc-dialog__surface">
  <header class="mdc-dialog__header">
    <h2 id="{{id}}-label" class="mdc-dialog__header__title">{{title}}</h2>
  </header>
  <section id="{{id}}-description" class="mdc-dialog__body {{if scrollable 'mdc-dialog__body--scrollable'}}">
    {{yield yieldDescription}}
  </section>
  <footer class="mdc-dialog__footer">{{yield yieldFooter}}</footer>
</div>
<div class="mdc-dialog__backdrop"></div>